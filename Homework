package com.company;

import java.util.Scanner;

class Date{
    private int day;
    private int month;
    private int year;

    public Date(int day,int month,int year){
        this.day=day;
        this.month=month;
        this.year=year;
    }
    public void setDay(int day){
            this.day=day;
    }
    public void setMonth(int month){
            this.month = month;
    }
    public void setYear(int year){
        this.year=year;
    }
    public int getDay(){
        return day;
    }
    public int getMonth(){
        return month;
    }
    public int getYear(){
        return year;
    }
    public void displayDate(){
        System.out.printf("%d/%d/%d",day,month,year);
    }
}
class DateTest{
    public static void main(String[] args) {
        Date Test=new Date(11,4,2020);
        System.out.print("the initial date:");
        Test.displayDate();
        Test.setDay(1);
        Test.setMonth(9);
        Test.setYear(2008);
        System.out.print("\nDate with new values is: ");
        Test.displayDate();
        System.out.println();
    }
}
